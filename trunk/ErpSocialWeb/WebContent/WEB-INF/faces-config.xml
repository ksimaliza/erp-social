<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
 <managed-bean>
  <managed-bean-name>estudianteDTO</managed-bean-name>
  <managed-bean-class>ec.edu.uce.erp.ejb.persistence.entity.matriculacion.EstudianteDTO</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>estCodigo</property-name>
   <property-class>java.lang.Integer</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>estPersona</property-name>
   <property-class>java.lang.Integer</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>matEstudianteRepresentantes</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>matMatriculas</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>serialVersionUID</property-name>
   <property-class>long</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <description>redirecionarHomeUsuario</description>
  <from-view-id>/seguridad/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>homeUsuario</from-outcome>
   <to-view-id>/paginas/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <description>redirecionarCambiarClave</description>
  <from-view-id>/seguridad/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cambiarClave</from-outcome>
   <to-view-id>/paginas/usuario/cambiarClave.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <description>retornarCambiarClaveUsuario</description>
  <navigation-case>
   <from-outcome>retornarCambiarClave</from-outcome>
   <to-view-id>/paginas/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <description>salirSistema</description>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/seguridad/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>ec.edu.uce.erp.web.common.resources.WebApplicationResources</base-name>
   <var>i18n</var>
  </resource-bundle>
  <locale-config>
   <default-locale>es</default-locale>
  </locale-config>
 </application>
</faces-config>
